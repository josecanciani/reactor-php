{
    "name": "josecanciani/reactor",
    "description": "Component based reactive framework",
    "type": "library",
    "authors": [
        {
            "name": "Jose Canciani",
            "email": "jose.canciani@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Reactor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reactor\\Demo\\": "demo/src/",
            "Reactor\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit tests/**/*.php"
        ],
        "test-one": [
            "vendor/bin/phpunit --"
        ]
    }
}
